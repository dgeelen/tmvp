{$M $800,0,0 }   { 2K stack, no heap }
uses Dos;

procedure GetAddresses(var pcode, pdata, pjump: pointer);
var
  tcode,tdata,tjump: word;
begin
  asm
    mov tcode, offset @lcode;
    mov tdata, offset @ldata;
    mov tjump, offset @ljump;
    mov word ptr cs:[offset @ldata], 1
    mov word ptr cs:[offset @ldata+2], 1
    mov word ptr cs:[offset @ldata+6], 1
    mov word ptr cs:[offset @ldata+8],  0
    mov word ptr cs:[offset @ldata+12], 0
    jmp @lend
  @lcode: { start of interrupt handler }
    cli
    push ax
    push bx
    mov bx, word ptr cs:[offset @ldata+8]
    cmp bx, word ptr cs:[offset @ldata+12]
    je @nosample
    mov al, byte ptr cs:[bx+offset @ldata+8064]
    out $42, al

    inc bx
    cmp bx, 16000
    jb @nowrap
    xor bx,bx
  @nowrap:
    mov word ptr cs:[offset @ldata+8],bx
  @nosample:

    pop bx
    dec word ptr cs:[offset @ldata]
    jz @doframe
    dec word ptr cs:[offset @ldata+2]
    jz @dochain
  @doeoi: { do end of interrupt (dont chain into orig) }
    mov al, $20
    out $20, al
    pop ax
    sti
    iret
  @noflip:
    push ax; push ds
    mov ax, $B800
    mov ds, ax
    mov ax,$0F60
    mov ds:[0000], ax
    mov ds:[0002], ax
    mov ds:[0004], ax
    mov ds:[0006], ax
    mov ds:[0008], ax
    pop ds
    pop ax
    jmp @enframe
  @doframe: { check if theres a frame to draw }
    mov word ptr cs:[offset @ldata], 671

    cmp byte ptr cs:[offset @ldata+6], 0
    jne @noflip

    push ax; push ds; push es; push di; push si; push cx
    push dx

{         MOV DX,03DAh

         @lopv0:    IN AL,DX
                 AND AL,8
                         JNZ @lopv0

           @lopv1:    IN AL,DX
                   AND AL,8
                           JZ @lopv1
}
  mov ax, cs
  mov ds, ax
  mov si, offset @ldata + 64 - 48
  mov cl, 0 { first 8 palette entries }
  xor cx, cx
  mov dx, 03C9h
@l1:
  mov al,cl  { al = color# }
  dec dx     { dx = 03C8h = select color }
  out dx, al { select color }
  inc dx     { dx = 03C9h = modify color }
  {Note: l00p unrolled for t3h sp33d }
  lodsb      { get a palette byte into al }
  out dx, al { output R }
  lodsb      { get a palette byte into al }
  out dx, al { output G }
  lodsb      { get a palette byte into al }
  out dx, al { output B }
  inc cl
  cmp cl, 6
  jne @l1

  mov al,20  { al = color# }
  dec dx     { dx = 03C8h = select color }
  out dx, al { select color }
  inc dx     { dx = 03C9h = modify color }
  {Note: l00p unrolled for t3h sp33d }
  lodsb      { get a palette byte into al }
  out dx, al { output R }
  lodsb      { get a palette byte into al }
  out dx, al { output G }
  lodsb      { get a palette byte into al }
  out dx, al { output B }

  inc cl
  mov al,cl  { al = color# }
  dec dx     { dx = 03C8h = select color }
  out dx, al { select color }
  inc dx     { dx = 03C9h = modify color }
  {Note: l00p unrolled for t3h sp33d }
  lodsb      { get a palette byte into al }
  out dx, al { output R }
  lodsb      { get a palette byte into al }
  out dx, al { output G }
  lodsb      { get a palette byte into al }
  out dx, al { output B }
  inc cl
  add cl, 48 { next 8 palette entries }
@l2:
  mov al,cl  { al = color# }
  dec dx     { dx = 03C8h = select color }
  out dx, al { select color }
  inc dx     { dx = 03C9h = modify color }
  {Note: l00p unrolled for t3h sp33d }
  lodsb      { get a palette byte into al }
  out dx, al { output R }
  lodsb      { get a palette byte into al }
  out dx, al { output G }
  lodsb      { get a palette byte into al }
  out dx, al { output B }
  inc cl
  cmp cl, 64
  jne @l2

    pop dx

    mov cx, 8000/4
    mov ax, $B800
    db 66h;mov es, ax
    xor di, di
{    mov ax, cs}
{    mov ds, ax}
    mov si, offset @ldata+64
    cld
    db 66h; rep movsw
    pop  cx; pop  si; pop  di; pop  es; pop  ds; pop  ax

    mov byte ptr cs:[offset @ldata+6], 1
  @enframe:
    dec word ptr cs:[offset @ldata+2]
    jnz @doeoi
  @dochain: { jump to old interrupt handler (chain) }
    pop ax
    mov word ptr cs:[offset @ldata+2], 885
  @ljump:
    { far jump to old handler }
    db $EA; db 4; db 3; db 2; db 1;
    { alignment bytes }
    db 0; db 0; db 0;
  @ldata:
